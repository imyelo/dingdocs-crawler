{
  "root": true,
  "extends": ["@yelo/eslint-config"],
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "no-console": "off",
    "import/extensions": "off",
    "no-await-in-loop": "off",
    "no-continue": "off"
  },
  "ignorePatterns": ["**/node_modules/*", "**/lib/*"]
}
